<h1>Questions#new</h1>
<p>Find me in app/views/questions/new.html.erb</p>

<h1>新規投稿</h1>
<!-- form_with部分 -->
<%= form_with model: @question, local:true do |f| %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>ジャンル</h4>
  <%#= f.text_field :question_genre %>

  <h4>本文</h4>
  <%= f.text_area :body %>

  <%= f.submit '投稿' %>

<% end %>