<h1>Questions#edit</h1>
<p>Find me in app/views/questions/edit.html.erb</p>

<%= form_with model:@question, local:true do |f| %>

<label for="inputTitle">Title</label>
<%= f.text_field :title, autofocus: true, id:"inputTitle", placeholder:"タイトル"%>

<label for="inputBody">内容</label>
<%= f.text_field :body, autofocus: true, id:"inputBody", placeholder:"内容"%>

<%= f.submit "変更を保存" %>
<% end %>